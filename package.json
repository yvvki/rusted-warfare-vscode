{
	"name": "rusted-warfare-vscode",
	"displayName": "Rusted Warfare Support",
	"description": "Workspace detection, syntax highlighting, code completion and format checker for Rusted Warfare",
	"version": "0.1.1",
	"homepage": "https://github.com/yvvki/rusted-warfare-vscode#readme",
	"bugs": {
		"url": "https://github.com/yvvki/rusted-warfare-vscode/issues"
	},
	"license": "MIT",
	"author": {
		"name": "Yuuki Rika",
		"email": "yvvki@duck.com"
	},
	"publisher": "yvvki",
	"repository": {
		"type": "git",
		"url": "https://github.com/yvvki/rusted-warfare-vscode.git"
	},
	"packageManager": "yarn@4.0.2",
	"engines": {
		"vscode": "^1.85.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "rw",
				"aliases": [
					"Rusted Warfare",
					"rw"
				],
				"extensions": [
					".txt",
					".ini",
					".template"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "rw",
				"scopeName": "source.ini.rw",
				"path": "./syntaxes/rw.tmLanguage.json"
			}
		]
	},
	"scripts": {
		"package": "yarn run vsce package --no-yarn --no-dependencies",
		"publish": "yarn run vsce publish --no-yarn --no-dependencies"
	},
	"devDependencies": {
		"@vscode/vsce": "^2.22.0",
		"xo": "^0.56.0"
	},
	"prettier": {
		"useTabs": true,
		"singleQuote": true,
		"bracketSpacing": false
	},
	"xo": {
		"prettier": true
	}
}
